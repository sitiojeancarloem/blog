{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:preact/recommended",
		"prettier"
	],
	"plugins": ["preact"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"tsx": true
		}
	},
	"rules": {
		"no-trailing-spaces": "error",
		"eol-last": ["error", "always"]
	}
}
